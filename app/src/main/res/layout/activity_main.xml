<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/scrollview"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#cc5a06">
    >

<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
android:layout_width="match_parent"
android:layout_height="match_parent"
android:background="#cc5a06">

        <LinearLayout
            android:layout_width="fill_parent"
            android:layout_height="fill_parent"
            android:orientation="vertical">



            <LinearLayout
                android:layout_width="fill_parent"
                android:layout_height="fill_parent"
                android:orientation="vertical">


                <LinearLayout
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal">

                    <LinearLayout
                        android:orientation="vertical"
                        android:layout_width="0dp"
                        android:layout_weight="1"
                        android:padding="10dip"
                        android:layout_height="120dp">



                        <ImageButton

                            android:id="@+id/imageButton1"
                            android:layout_height="match_parent"
                            android:adjustViewBounds="true"
                            android:scaleType="fitCenter"
                            android:layout_gravity="center"
                            android:layout_width="match_parent"
                            android:layout_row="0"
                            android:onClick="openRoutineDept"
                            android:layout_column="0"
                            android:outlineProvider="paddedBounds"
                            android:visibility="visible"
                            android:cropToPadding="false"
                            android:background="@drawable/a" />
                     </LinearLayout>


                    <LinearLayout
                        android:orientation="vertical"
                        android:layout_width="0dp"
                        android:layout_weight="1"
                        android:padding="10dip"
                        android:layout_height="120dp">


                        <ImageButton
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:id="@+id/imageButton2"
                            android:layout_row="2"
                            android:layout_column="0"
                            android:background="@drawable/attendance"
                            android:layout_gravity="center"
                            android:onClick="viewattendance"
                            android:layout_below="@+id/imageButton1"
                            android:layout_centerHorizontal="true" />
                    </LinearLayout>



                    <LinearLayout
                        android:orientation="vertical"
                        android:layout_width="0dp"
                        android:layout_weight="1"
                        android:padding="10dip"
                        android:layout_height="120dp">


                        <ImageButton
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:id="@+id/imageButton"
                            android:layout_row="2"
                            android:layout_column="0"
                            android:background="@drawable/bus"
                            android:layout_centerHorizontal="true"
                            android:onClick="openBus"
                            android:layout_gravity="center" />
                    </LinearLayout>



                </LinearLayout>

                <LinearLayout
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal" >

                    <LinearLayout
                        android:orientation="vertical"
                        android:layout_width="0dp"
                        android:layout_weight="1"
                        android:padding="10dip"
                        android:layout_height="120dp" >

                        <ImageButton
                            android:id="@+id/imageButton4"
                            android:layout_height="match_parent"
                            android:adjustViewBounds="true"
                            android:scaleType="fitCenter"
                            android:layout_gravity="center"
                            android:layout_width="match_parent"
                            android:layout_row="0"
                            android:layout_column="0"
                            android:outlineProvider="paddedBounds"
                            android:visibility="visible"
                            android:cropToPadding="false"
                            android:onClick="canteen"
                            android:background="@drawable/canteen" />
                    </LinearLayout>

                    <LinearLayout
                        android:orientation="vertical"
                        android:layout_width="0dp"
                        android:layout_weight="1"
                        android:padding="10dip"
                        android:layout_height="120dp" >

                        <ImageButton
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:id="@+id/imageButton5"
                            android:layout_row="2"
                            android:layout_column="0"
                            android:background="@drawable/exam"
                            android:layout_gravity="center"
                            android:onClick="exam"
                            android:layout_below="@+id/imageButton3"
                            android:layout_centerHorizontal="true" />
                    </LinearLayout>

                    <LinearLayout
                        android:orientation="vertical"
                        android:layout_width="0dp"
                        android:layout_weight="1"
                        android:padding="10dip"
                        android:layout_height="120dp" >

                        <ImageButton
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:id="@+id/imageButton6"
                            android:layout_row="2"
                            android:layout_column="0"
                            android:background="@drawable/lib"
                            android:onClick="library"
                            android:layout_below="@+id/imageButton3"
                            android:layout_centerHorizontal="true"
                            android:layout_gravity="center" />
                    </LinearLayout>
                </LinearLayout>

                <LinearLayout
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal" >

                    <LinearLayout
                        android:orientation="vertical"
                        android:layout_width="0dp"
                        android:layout_weight="1"
                        android:padding="10dip"
                        android:layout_height="120dp" >

                        <ImageButton
                            android:id="@+id/imageButton7"
                            android:layout_height="match_parent"
                            android:adjustViewBounds="true"
                            android:scaleType="fitCenter"
                            android:layout_gravity="center"
                            android:layout_width="match_parent"
                            android:layout_row="0"
                            android:layout_column="0"
                            android:onClick="notice"
                            android:outlineProvider="paddedBounds"
                            android:visibility="visible"
                            android:cropToPadding="false"
                            android:background="@drawable/nts" />
                    </LinearLayout>

                    <LinearLayout
                        android:orientation="vertical"
                        android:layout_width="0dp"
                        android:layout_weight="1"
                        android:padding="10dip"
                        android:layout_height="120dp" >

                        <ImageButton
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:id="@+id/imageButton8"
                            android:layout_row="2"
                            android:layout_column="0"
                            android:onClick="openTeachers"
                            android:layout_gravity="center"
                            android:layout_below="@+id/imageButton3"
                            android:layout_centerHorizontal="true"
                            android:background="@drawable/ts" />
                    </LinearLayout>

                    <LinearLayout
                        android:orientation="vertical"
                        android:layout_width="0dp"
                        android:layout_weight="1"
                        android:padding="10dip"
                        android:layout_height="120dp" >

                        <ImageButton
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:id="@+id/imageButton9"
                            android:layout_row="2"
                            android:layout_column="0"
                            android:onClick="ebooks"
                            android:layout_below="@+id/imageButton3"
                            android:layout_centerHorizontal="true"
                            android:layout_gravity="center"
                            android:background="@drawable/ebook" />
                    </LinearLayout>
                </LinearLayout>

                <LinearLayout
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal" >

                    <LinearLayout
                        android:orientation="vertical"
                        android:layout_width="0dp"
                        android:layout_weight="1"
                        android:padding="10dip"
                        android:layout_height="120dp" >

                        <ImageButton
                            android:id="@+id/imageButton10"
                            android:layout_height="match_parent"
                            android:adjustViewBounds="true"
                            android:scaleType="fitCenter"
                            android:layout_gravity="center"
                            android:layout_width="match_parent"
                            android:layout_row="0"
                            android:layout_column="0"
                            android:outlineProvider="paddedBounds"
                            android:visibility="visible"
                            android:onClick="notes"
                            android:cropToPadding="false"


                            android:background="@drawable/notes" />
                    </LinearLayout>

                    <LinearLayout
                        android:orientation="vertical"
                        android:layout_width="0dp"
                        android:layout_weight="1"
                        android:padding="10dip"
                        android:layout_height="120dp" >

                        <ImageButton
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:id="@+id/imageButton11"
                            android:layout_row="2"
                            android:layout_column="0"
                            android:layout_gravity="center"
                            android:layout_below="@+id/imageButton3"
                            android:layout_centerHorizontal="true"
                            android:onClick="samplequestions"
                            android:background="@drawable/sq" />
                    </LinearLayout>

                    <LinearLayout
                        android:orientation="vertical"
                        android:layout_width="0dp"
                        android:layout_weight="1"
                        android:padding="10dip"
                        android:layout_height="120dp" >

                        <ImageButton
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:id="@+id/imageButton12"
                            android:layout_row="2"
                            android:layout_column="0"
                            android:layout_below="@+id/imageButton3"
                            android:layout_centerHorizontal="true"
                            android:layout_gravity="center"
                            android:onClick="lessonplans"
                            android:background="@drawable/lp" />
                    </LinearLayout>
                </LinearLayout>

            </LinearLayout>
    </LinearLayout>



</FrameLayout>


    </ScrollView>
